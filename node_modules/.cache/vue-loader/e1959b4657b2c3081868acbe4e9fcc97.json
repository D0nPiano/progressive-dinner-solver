th > 2\"\n        :data=\"csv\"\n        :columns=\"mapFields\"\n      >\n        <template slot=\"submit\" slot-scope=\"{submit}\">\n          <button @click.prevent=\"submit\">send!</button>\n        </template>\n      </v-client-table>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { VueCsvImport } from \"vue-csv-import\";\n\nexport default {\n  name: \"Teams\",\n  components: { VueCsvImport },\n  data: function() {\n    return {\n      csv: [\n        {\n          Vorname: \"test\",\n          Nachname: \"test\",\n          Organisation: \"test\",\n          Küche: \"test\"\n        }\n      ],\n      küchenMap: {\n        \"Ja\": -1,\n        \"Nein\": 1,\n        \"Wenn es nicht anders geht\": 0\n      },\n      mapFields: ['Vorname', 'Straße',\t'Hausnummer',\t'PLZ', 'Küche']\n      //mapFields: ['Vorname',\t'Nachname',\t'E-Mail',\t'Telefon',\t'Alter',\t'Straße',\t'Hausnummer',\t'PLZ',\t'Viertel',\t'Organisation',\t'Küche',\t'Allergien',\t'Anmerkungen',\t'Datum']\n    };\n  },\n  methods: {\n    searchLocation() {\n      axios\n      .get('https://api.coindesk.com/v1/bpi/currentprice.json')\n      .then(response => (this.info = response))\n    },\n    onSubmit() {\n      this.mapFields.push('TeamId')\n      this.mapFields.push('KücheBenutzt')\n      this.csv.sort((x, y) => {\n          return this.küchenMap[x.Küche] - this.küchenMap[y.Küche];\n        }\n      );\n      let numberOfTeams = this.csv.length / 2\n      let currentTeamId = 1\n      let kücheBenutzt = true\n      this.csv.forEach(x => {\n        x.TeamId = currentTeamId\n        x.KücheBenutzt = kücheBenutzt ? \"Ja\" : \"Nein\"\n        currentTeamId += 1\n        if (currentTeamId > numberOfTeams) {\n          currentTeamId = 1\n          kücheBenutzt = false\n        }\n      });\n      this.$store.commit(\"setCsv\", this.csv);\n    },\n    exportCsv() {\n\n    }\n  }\n};\n</script>\n\n<style>\ntable {\n  border-collapse: collapse;\n  width: 100%;\n}\n\nth,\ntd {\n  text-align: left;\n  padding: 8px;\n}\n\ntr:nth-child(even) {\n  background-color: #f2f2f2;\n}\n</style>\n"]}]}